{
    "name": "phpbbservices/notifyonerror",
    "type": "phpbb-extension",
    "description": "Notifies administrators when an entry is made to the phpBB error log",
    "homepage": "https://www.phpbbservices.com/my-software/notify-on-error/",
    "version": "1.0.2-beta",
    "time": "2022-08-06",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Mark D. Hamill",
            "email": "mark@phpbbservices.com",
            "homepage": "https://www.phpbbservices.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3,<=8.1.*",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Notify admins on new error log entries",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<3.4.0@dev"
        },
        "version-check": {
            "host": "www.phpbbservices.com",
            "directory": "/other/notifyonerror/",
            "filename": "3.3.json",
            "ssl": true
        }
    }
}
